{
  "hosting": {
    "public": "web/.vuepress/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/v1",
        "function": "tide"
      },
      {
        "source": "/api/v1/**",
        "function": "tide"
      },
      {
        "source": "/api/spec/v1",
        "function": "docs"
      },
      {
        "source": "/api/spec/v1/**",
        "function": "docs"
      }
    ]
  }
}
